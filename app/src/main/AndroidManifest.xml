<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.icbc">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icbc"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icbc"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".CalculateDepositIncomeActivity"
            android:label="@string/title_activity_calculate_deposit_income"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CalculateLoanIncomeActivity"
            android:label="@string/title_activity_calculator_loan_income"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>